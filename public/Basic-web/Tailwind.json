    },
    "cursor": {
      "description": "Sets the type of cursor.",
      "values": ["cursor-auto", "cursor-default", "cursor-pointer", "cursor-wait", "cursor-text", "cursor-move", "cursor-not-allowed"]
    },
    "position": {
      "description": "Sets the positioning of an element.",
      "values": ["static", "relative", "absolute", "fixed", "sticky"]
