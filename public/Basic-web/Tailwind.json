    "top": {
      "description": "Sets the top position of a positioned element.",
      "values": ["top-{size}", "top-0", "top-1/2", "top-full"]
    },
    "right": {
      "description": "Sets the right position of a positioned element.",
      "values": ["right-{size}", "right-0", "right-1/2", "right-full"]
    },
    "bottom": {
      "description": "Sets the bottom position of a positioned element.",
      "values": ["bottom-{size}", "bottom-0", "bottom-1/2", "bottom-full"]
    },
    "left": {
      "description": "Sets the left position of a positioned element.",
      "values": ["left-{size}", "left-0", "left-1/2", "left-full"]
    }
